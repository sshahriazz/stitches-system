{
  "name": "stitches-system",
  "version": "1.0.1",
  "description": "css-in-js design system based on stitches js.",
  "homepage": "https://github.com/sshahriazz/stitches-system",
  "author": {
    "name": "Sharieaz Kavier",
    "email": "shahriazkobir@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/sshahriazz/stitches-system/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sshahriazz/stitches-system"
  },
  "keywords": [
    "next",
    "next ui",
    "components",
    "modern components",
    "react components",
    "react ui"
  ],
  "files": [
    "lib",
    "dist",
    "es"
  ],
  "source": "src/index.ts",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "unpkg": "lib/umd/tnextui.min.js",
  "types": "lib/types/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "scripts": {
    "clear": "rimraf ./lib ./dist ./esm",
    "clear-types": "rimraf ./lib/types",
    "pre-publish": "node ./scripts/pre-publish.js",
    "publish:dry-run": "npm publish ./lib --dry-run",
    "build": "node ./scripts/build.js",
    "build:minify": "node ./scripts/minify.js",
    "build:types": "yarn tsc -p ./buildconfig -d --emitDeclarationOnly --outDir types",
    "build:dev-types": "tsc -p ./buildconfig -d --emitDeclarationOnly --outDir lib/esm",
    "build:webpack": "webpack --config ./buildconfig/webpack.common.js",
    "build:watch": "node ./scripts/build-watch.js",
    "dev": "yarn clear && yarn build:dev-types && yarn build:watch",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss,css,json}'"
  },
  "publishConfig": {
    "directory": "lib"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "@stitches/react": "1.2.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@react-bootstrap/babel-preset": "^2.1.0",
    "@react-types/shared": "^3.15.0",
    "@types/node": "^15.12.4",
    "@types/react": "17.0.33",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-optimize-clsx": "^2.6.2",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "chalk": "^4.1.2",
    "cherry-pick": "^0.5.0",
    "css-mediaquery": "^0.1.2",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "p-iteration": "^1.1.8",
    "prettier": "^2.3.1",
    "react": "17.0.2",
    "rimraf": "^3.0.2",
    "terser": "5.10.0",
    "typescript": "4.6.2",
    "webpack": "^5.53.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^5.8.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-ts-lambdas": "^1.2.3",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.1"
  }
}
